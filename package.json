{
   "name": "ulesson-test",
   "version": "1.0.0",
   "description": "uLesson Snr. Backed Eng Test",
   "main": "app.js",
   "scripts": {
      "debug": "node --inspect=0.0.0.0 index.js",
      "debug-tests": "mocha --inspect-brk=0.0.0.0:5859 --opts test/mocha.debug.opts './test/index.js'",
      "pre-dev-start": "db-migrate up --env dev",
      "dev-start": "npm run pre-dev-start && nodemon --signal SIGINT --inspect=0.0.0.0:5858 app.js",
      "prestart": "db-migrate up --env prod",
      "start": "node app.js",
      "test": "nyc --reporter=html --reporter=text mocha --config=test/.mocharc.json --no-deprecation './test/index.js'",
      "test-no-coverage": "mocha --config=test/.mocharc.json --no-deprecation './test/index.js'",
      "docs-coverage": "esdoc",
      "synk": "synk test",
      "prepare": "husky install",
      "lint": "eslint .",
      "migrate": "npx db-migrate up -e prod",
      "migrate-dev": "npx db-migrate up -e dev",
      "migrate-dry-run": "npx db-migrate up -e prod --dry-run",
      "migrate-dev-dry-run": "npx db-migrate up -e dev --dry-run",
      "rollback": "npx db-migrate down -e prod",
      "rollback-dev": "npx db-migrate down -e dev",
      "rollback-dry-run": "npx db-migrate down -e prod --dry-run",
      "rollback-dev-dry-run": "npx db-migrate down -e dev --dry-run",
      "create-migration": "npx db-migrate create $npm_config_name -e prod --sql-file",
      "create-dev-migration": "npx db-migrate create $npm_config_name -e dev --sql-file"
   },
   "repository": {
      "type": "git",
      "url": "git+https://github.com/django102/uLesson-test.git"
   },
   "keywords": [
      "uLesson"
   ],
   "author": "django102",
   "license": "MIT",
   "bugs": {
      "url": "https://github.com/django102/uLesson-test/issues"
   },
   "homepage": "https://github.com/django102/uLesson-test#readme",
   "dependencies": {
      "bcryptjs": "^2.4.3",
      "body-parser": "^1.20.2",
      "chance": "^1.1.11",
      "console.log": "^0.1.3",
      "cookie-parser": "^1.4.6",
      "cors": "^2.8.5",
      "crypto": "^1.0.1",
      "db-migrate": "^0.11.14",
      "db-migrate-mysql": "^2.3.2",
      "dotenv": "^16.3.1",
      "express": "^4.18.2",
      "express-pino-logger": "^7.0.0",
      "express-rate-limit": "^6.11.2",
      "form-data": "^4.0.0",
      "joi": "^17.9.2",
      "jsonwebtoken": "^9.0.1",
      "lodash": "^4.17.21",
      "method-override": "^3.0.0",
      "moment": "^2.29.4",
      "moment-timezone": "^0.5.43",
      "multer": "^1.4.5-lts.1",
      "mysql2": "^3.5.2",
      "pino": "^8.14.2",
      "pino-pretty": "^10.2.0",
      "reflect-metadata": "^0.1.14",
      "sanitize": "^2.1.2",
      "sequelize": "^6.32.1",
      "typeorm": "^0.3.17",
      "uniqid": "^5.4.0",
      "util": "^0.12.5",
      "uuid": "^9.0.0"
   },
   "devDependencies": {
      "@babel/eslint-parser": "^7.22.9",
      "babel-eslint": "^10.1.0",
      "babel-plugin-istanbul": "^6.1.1",
      "barrels": "^0.0.5",
      "esdoc": "^1.1.0",
      "esdoc-standard-plugin": "^1.0.0",
      "eslint": "^8.46.0",
      "eslint-config-airbnb-base": "^15.0.0",
      "eslint-config-prettier": "^9.0.0",
      "eslint-config-standard": "^17.1.0",
      "eslint-plugin-import": "^2.28.0",
      "eslint-plugin-n": "^16.0.1",
      "eslint-plugin-no-only-tests": "^3.1.0",
      "eslint-plugin-node": "^11.1.0",
      "eslint-plugin-prettier": "^5.0.0",
      "eslint-plugin-promise": "^6.1.1",
      "eslint-plugin-you-dont-need-lodash-underscore": "^6.12.0",
      "husky": "^8.0.3",
      "lint-staged": "^13.2.3",
      "mocha": "^10.2.0",
      "nyc": "^15.1.0",
      "prettier": "3.0.1",
      "should": "^13.2.3",
      "sinon": "^15.2.0",
      "supertest": "^6.3.3"
   },
   "lint-staged": {
      "**/*": "prettier --write . --ignore-unknown"
   }
}
